{
	"name": "@menus/backoffice-spa",
	"description": "menu.com backoffice spa",
	"version": "1.22.0",
	"type": "module",
	"bin": {
		"APP_VERSION": "./bin/APP_VERSION.mjs",
		"package-zip": "./bin/package-zip.mjs"
	},
	"scripts": {
		"APP_VERSION": "./bin/APP_VERSION.mjs",
		"build": "npm run clean && APP_VERSION=$(npm run APP_VERSION | tail -n1) DEV=1 npm run compile && APP_VERSION=$(npm run APP_VERSION | tail -n1) LIVE=1 npm run compile ",
		"build-watch": "rollup -c rollup.config.mjs -w",
		"clean": "rm -rf dist",
		"compile": "rollup -c rollup.config.mjs",
		"cy:run": "cypress run",
		"cy:open": "cypress open",
		"dev": "vite dev",
		"package": "APP_VERSION=$(cat dist/live/dist/APP_VERSION) ./bin/package-zip.mjs",
		"precommit": "./bin/build_APP_VERSION.mjs",
		"postversion": "./bin/build_APP_VERSION.mjs",
		"start": "node build",
		"test": "run-p --race dev cy:run"
	},
	"devDependencies": {
		"@ctx-core/object": "^21.0.21",
		"@menus/adapter_file": "workspace:^1.0.1",
		"@menus/bootstrap": "workspace:^1.2.13",
		"@menus/consumer-shared-css": "workspace:^1.1.1",
		"@menus/css": "workspace:^1.1.4",
		"@menus/http": "workspace:^1.3.7",
		"@menus/import-meta-resolve": "workspace:^1.0.1",
		"@menus/ro-layout-ui": "workspace:^1.2.38",
		"@menus/ro-login-ui": "workspace:^1.1.10",
		"@menus/ro-shared-css": "workspace:^1.1.1",
		"@menus/shared-ui": "workspace:^1.5.9",
		"@menus/ui": "workspace:^1.4.3",
		"@menus/web": "workspace:^2.115.5",
		"@menus/web-build": "workspace:^1.1.8",
		"@menus/web-config": "workspace:^1.6.14",
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-replace": "^3.0.0",
		"@rollup/plugin-typescript": "^8.3.0",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@sveltejs/vite-plugin-svelte": "next",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.106",
		"bootstrap-sass": "^3.4.1",
		"child-process-promise": "^2.2.1",
		"rollup": "^2.59.0",
		"rollup-plugin-combine": "^2.1.1",
		"rollup-plugin-css-only": "^3.1.0",
		"rollup-plugin-esbuild": "^4.6.0",
		"rollup-plugin-node-globals": "^1.4.0",
		"rollup-plugin-svelte": "^7.1.0",
		"sapper": "workspace:^0.29.1",
		"svelte": "^3.44.2",
		"svelte-preprocess": "^4.9.8",
		"swiper": "^6.8.4"
	},
	"noUpdate": [
		"swiper"
	]
}
